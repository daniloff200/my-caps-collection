{
  "project": {
    "version": "1.0.0",
    "name": "tdp-candidates"
  },
  "apps": [
    {
      "root": "demo/src",
      "outDir": "demo/dist",
      "assets": [
      ],
      "index": "index.html",
      "main": "main.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.json",
      "testTsconfig": "../../src/tsconfig.spec.json",
      "polyfills": "polyfills.ts",
      "prefix": "tdp",
      "mobile": false,
      "styles": [
      ],
      "scripts": [
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "addons": [],
  "packages": [],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "files": "src/**/*.ts",
      "project": "src/tsconfig.spec.json"
    },
    {
      "files": "src/**/*.ts",
      "project": "src/tsconfig.json"
    },
    {
      "files": "e2e/**/*.ts",
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "polyfills": "polyfills.ts",
  "test": {
    "codeCoverage": {
      "exclude": [
        "**/polyfills.ts",
        "**/test.ts",
        "**/*.mock.ts",
        "**/*.sample.ts",
        "**/testing/*/*.ts",
        "**/testing/*.ts",
        "**/index.ts"
      ]
    },
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "prefixInterfaces": true,
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}
